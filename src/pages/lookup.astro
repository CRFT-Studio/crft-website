            import $ from 'jquery';

            $(document).ready(function () {
                $("#metaTitle").on("input", () => {
                    $("#titleLength").text($("#metaTitle").val().length);
                });
                $("#metaDescription").on("input", () => {
                    $("#descriptionLength").text($("#metaDescription").val().length);
                });
            });
        </script>
    </head>

