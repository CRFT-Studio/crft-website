import Development from '../../src/images/development.png';
import ClientCard from '../components/ClientCard.astro';
import MobiLoudDesktop from '../../src/images/mobiloud-desktop.png';
import FeedBearDesktop from '../../src/images/feedbear-desktop.png';
import HoppyCopyDesktop from '../../src/images/hoppy-copy-desktop.png';
